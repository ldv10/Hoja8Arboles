class NodoH extends ArbolH {
    private  ArbolH izquierda = null, derecha = null; // Subdivision de arboles
 
    public NodoH(ArbolH i, ArbolH d) {
        super(i.getFrecuencia() + d.getFrecuencia());
        izquierda = i;
        derecha = d;
    }

	public ArbolH getIzquierda() {
		return izquierda;
	}

	public void setIzquierda(ArbolH izquierda) {
		this.izquierda = izquierda;
	}

	public ArbolH getDerecha() {
		return derecha;
	}

	public void setDerecha(ArbolH derecha) {
		this.derecha = derecha;
	}
    
    
}
