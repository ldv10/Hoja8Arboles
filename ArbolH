import java.util.*;
 
abstract class ArbolH implements Comparable<ArbolH> {
    private  int frecuencia; // Frecuencia del arbol
    
    public int getFrecuencia() {
		return frecuencia;
	}

	public void setFrecuencia(int frecuencia) {
		this.frecuencia = frecuencia;
	}

	public ArbolH (int freq) { 
    	frecuencia = freq;
    	}
 
    //Compara
    public int compareTo(ArbolH tree) {
        return frecuencia - tree.frecuencia;
    }
}

