class HojaH extends ArbolH {
    private  char valor; //El valor que se representa en la hoja
 
    public HojaH (int freq, char val) {
        super(freq);
        valor = val;
    }

	public char getValor() {
		return valor;
	}

	public void setValor(char valor) {
		this.valor = valor;
	}
}
