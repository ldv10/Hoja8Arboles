//Esta clase se encarga de representar un arbol que no tiene hijos.
public class HojaH extends ArbolH {
    private  char valor; //El valor que se representa en la hoja
 
 	// Constructor
    public HojaH (int frequencia, char val) {
        super(frequencia);
        valor = val;
    }
    
    
// Getters y Setters
	public char getValor() {
		return valor;
	}

	public void setValor(char valor) {
		this.valor = valor;
	}

	
}
